{
  "name": "@hexonet/ispapi-apiconnector",
  "description": "Node.js SDK for the insanely fast HEXONET API",
  "version": "5.5.1",
  "private": false,
  "author": {
    "name": "Kai Schwarz",
    "email": "kschwarz@hexonet.net"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "homepage": "https://github.com/hexonet/node-sdk",
  "repository": "github:hexonet/node-sdk",
  "bugs": "https://github.com/hexonet/node-sdk/issues",
  "maintainers": [
    {
      "name": "Kai Schwarz",
      "email": "kschwarz@hexonet.net"
    }
  ],
  "keywords": [
    "hexonet",
    "domain",
    "api",
    "connector",
    "isp",
    "ispapi",
    "ssl",
    "cert",
    "dns",
    "backorder",
    "premium",
    "preregistration",
    "registration",
    "application"
  ],
  "nyc": {
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "**/*.spec.js",
      "test-check-helper.js",
      "test-commands.js"
    ]
  },
  "scripts": {
    "build": "tsc --version && tsc",
    "documentation": "npm run documentation_docco --silent && npm run documentation_typedoc --silent",
    "documentation_docco": "docco -l parallel -o docs/docco src/*.ts",
    "documentation_typedoc": "typedoc --out docs/typedoc --name @hexonet/ispapi-apiconnector --readme ./README.md --mode file --includeDeclarations --excludeExternals src/*.ts",
    "lint": "eslint --fix --config=./.eslintrc.js *.js README.md *.json test/*.spec.js",
    "test": "cross-env NODE_ENV=development nyc ./node_modules/.bin/_mocha $(find ./test -type f -name '*.spec.js')"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/npm": "^5.1.15",
    "@types/request": "^2.48.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^6.0.0",
    "dirty-chai": "^2.0.1",
    "docco": "^0.8.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.3",
    "jsdoc-oblivion": "^0.0.9",
    "mocha": "^6.1.4",
    "nock": "^11.3.3",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.24",
    "supertest": "^4.0.2",
    "tslint": "^5.19.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "request": "^2.88.0"
  }
}