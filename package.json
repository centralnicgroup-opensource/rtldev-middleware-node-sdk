{
  "name": "ispapi-apiconnector",
  "description": "1API API Connector for node.js",
  "version": "4.1.0",
  "author": {
    "name": "Kai Schwarz",
    "email": "kschwarz@1api.net"
  },
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=0.10.36"
  },
  "homepage": "http://1api.net",
  "maintainers": [
    {
      "name": "Kai Schwarz",
      "email": "kschwarz@1api.net"
    }
  ],
  "keywords": [
    "1api",
    "domain",
    "api",
    "connector",
    "isp",
    "ispapi",
    "ssl",
    "cert",
    "dns",
    "backorder",
    "premium",
    "preregistration",
    "registration",
    "application"
  ],
  "nyc": {
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "**/*.spec.js",
      "test-check-helper.js",
      "test-commands.js"
    ]
  },
  "scripts": {
    "version": "auto-changelog --package --output HISTORY.md; git add HISTORY.md",
    "test": "npm run test-NODE --silent",
    "test-NODE": "cross-env NODE_ENV=development nyc ./node_modules/.bin/_mocha --opts mocha.opts $(find . -type f -name '*.spec.js'  ! -path './nyc_output/*' ! -path './coverage/*' ! -path './node_modules/*')"
  },
  "devDependencies": {
    "auto-changelog": "0.3.3",
    "chai": "4.1.1",
    "cross-env": "5.0.5",
    "istanbul": "0.4.5",
    "mocha": "3.5.0",
    "nock": "9.0.14",
    "nyc": "11.1.0"
  },
  "dependencies": {}
}
